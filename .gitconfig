[user]
	name = Stefan Kasberger
	email = stefan.kasberger@alpine-geckos.at

[alias]
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all 
    st = status	
[credential]
	helper = cache --timeout=3600
[core]
	excludesfile = /home/cheeseman/.gitignore_global
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
